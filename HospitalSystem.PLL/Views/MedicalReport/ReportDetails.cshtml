@using HospitalSystem.DAL.Entity
@model MedicalReport


<h2>Your Medical Report</h2>
@if (Model.ReportImagePath != null)
{
     <img src="~/ProfileImage/reports/@Model.ReportImagePath"  alt="Medical Report Image" style="display: block; margin-left: auto; margin-right: auto; width: 1250px; height: 800px;">
}
else
{
    <p>No report image found.</p>
}
